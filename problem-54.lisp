(defun make-expected-list ()
  (loop for (hand1 hand2) across (read-problem-n-data 54) 
       for i = 1 then (1+ i) collect
       (list i (get-hand-type hand1) (get-hand-type hand2) (if (texas-holdem> hand1 hand2) :hand1 :hand2))))

(defun check-expectation-for-game (expected-outcomes game-number hand1 hand2)
  (let ((data (find-if #'(lambda (x) (= (first x) game-number)) expected-outcomes)))
    (when data
      (destructuring-bind (expected-hand1-type expected-hand2-type expected-winner) (rest data)
	(when (not (eq expected-hand1-type (get-hand-type hand1)))
	  (format t "game ~a: hand1 expected type is '~a' but got '~a'~%" game-number expected-hand1-type (get-hand-type hand1)))
	(when (not (eq expected-hand2-type (get-hand-type hand2)))
	  (format t "game ~a: hand2 expected type is '~a' but got '~a'~%" game-number expected-hand2-type (get-hand-type hand2)))
	(let ((winner (if (texas-holdem> hand1 hand2) :hand1 :hand2)))
	  (when (not (eq winner expected-winner))
	    (format t "game ~a: expected winner is '~a' but got '~a'~%" game-number expected-winner winner)))))))
(defun check-expectations-54 ()
  (let ((expected-outcomes (p-54-expected))
	(checked 0))
    (loop for (hand1 hand2) across (read-problem-n-data 54) 
       for game-number = 1 then (1+ game-number) do	 
	 (check-expectation-for-game expected-outcomes game-number hand1 hand2)
	 (incf checked))
    (format nil "~a checked" checked)))
	 